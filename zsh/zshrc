CURRENT_DIRECTORY=`dirname $(readlink ~/.zshrc)`
export ZSH="/Users/paarthmadan/.oh-my-zsh"

ZSH_DISABLE_COMPFIX=true

plugins=(git z)
source $ZSH/oh-my-zsh.sh
DEFAULT_USER="paarthmadan"
stty erase '^?'

fpath+=$HOME/.zsh/pure

autoload -U promptinit; promptinit
prompt pure

